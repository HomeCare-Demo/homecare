# terraform.tfvars.example
# Copy this file to terraform.tfvars and customize the values

# Azure Authentication (required)
appId          = "your-azure-app-id"
password       = "your-azure-app-secret"
tenantId       = "your-azure-tenant-id"  
subscriptionId = "your-azure-subscription-id"

# Note: GitHub token should be set as GITHUB_TOKEN environment variable in Terraform Cloud

# Basic Configuration
resource_group_name = "homecare"
location           = "Central India"
prefix             = "homecare"
cluster_name       = "homecare"
app_name          = "homecare-app"

# GitHub Repository (REQUIRED - Update this with your repository)
github_repo = "homecare-demo/homecare"

# Tags for all resources
tags = {
  Project     = "HomeCare"
  Environment = "Production"
  ManagedBy   = "Terraform"
  Owner       = "homecare-demo"
}
